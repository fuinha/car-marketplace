{
  "name": "market",
  "description": "Prodigy Marketplace",
  "author": "Kenzan Boo <kenzan@getprodigy.com>",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/getprodigy/market"
  },
  "main": "bin/server.js",
  "scripts": {
    "build": "better-npm-run build",
    "postinstall": "npm run build",
    "lint": "node_modules/eslint/bin/eslint.js -c .eslintrc src",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "test": "karma start",
    "intern": "intern-runner config=test/config/intern",
    "intern-server": "chromedriver --port=4444 --url-base=wd/hub",
    "intern-local": "NODE_ENV=local intern-runner config=test/config/intern.local"
  },
  "betterScripts": {
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 4000,
        "APIPORT": 8888
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 4000,
        "APIPORT": 8888
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js"
    }
  },

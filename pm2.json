{
  "apps"          : [{
    "name"        : "market",
    "script"      : "bin/server.js",
    "cwd"         : "/home/node/market/current",
    "env"         : {
      "DEBUG"     : "market*",
      "DEBUG_FD"  : "1",
      "NODE_ENV"  : "production",
